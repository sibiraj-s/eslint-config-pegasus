{
  "name": "eslint-config-pegasus",
  "version": "3.0.0",
  "description": "Eslint shareable config with personalized defaults",
  "author": "sibiraj-s",
  "license": "MIT",
  "repository": "https://github.com/sibiraj-s/eslint-config-pegasus.git",
  "bugs": "https://github.com/sibiraj-s/eslint-config-pegasus/issues",
  "homepage": "https://github.com/sibiraj-s/eslint-config-pegasus#readme",
  "engines": {
    "node": ">=16"
  },
  "main": "./index.js",
  "exports": {
    ".": "./index.js",
    "./node": "./node.js",
    "./typescript": "./typescript.js"
  },
  "scripts": {
    "validate": "eslint . --ext .js,.ts",
    "test": "npm run validate",
    "prepublishOnly": "npm run test",
    "prepare": "is-ci || husky install"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "pegasus"
  ],
  "files": [
    "CHANGELOG.md",
    "rules",
    "node.js",
    "typescript.js",
    "index.js"
  ],
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "confusing-browser-globals": "^1.0.10",
    "eslint-config-ts-compat": "^1.0.2",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "eslint": "^8.0.1",
    "husky": "^7.0.2",
    "is-ci": "^3.0.1",
    "lint-staged": "^11.2.3"
  }
}
